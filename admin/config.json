{
  "backend": {
    "name": "github",
    "repo": "WhyCarb/whycarb-site",
    "branch": "main"
  },
  "media_folder": "images/uploads",
  "public_folder": "/images/uploads",
  "collections": [
    {
      "name": "pages",
      "label": "Pages",
      "files": [
        {
          "file": "index.md",
          "label": "Home Page",
          "name": "home",
          "fields": [
            { "label": "Title", "name": "title", "widget": "string" },
            { "label": "Body", "name": "body", "widget": "markdown" }
          ]
        }
      ]
    }
  ]
}
